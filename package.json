{
  "name": "tasks-microservice",
  "version": "1.0.0",
  "description": "A microservice for asynchronous task processing with worker threads",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "watch": "nodemon --exec ts-node src/server.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "microservice",
    "tasks",
    "worker-threads",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "proper-lockfile": "^4.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
